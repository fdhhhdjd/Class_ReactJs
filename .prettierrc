{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "endOfLine": "lf",
  "semi": true,
  "tabWidth": 4,
  "useTabs": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "bracketSpacing": true,
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "singleQuote": false,
        "tabWidth": 4
      }
    }
  ]
}

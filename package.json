{
  "name": "e-commerce",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "build-dev": "VITE_NODE_ENV=dev env-cmd -f .env.${VITE_NODE_ENV} vite build",
    "build-staging": "VITE_NODE_ENV=staging env-cmd -f .env.${VITE_NODE_ENV} vite build",
    "build-pro": "VITE_NODE_ENV=pro env-cmd -f .env.${VITE_NODE_ENV} vite build",
    "start": "env-cmd -f .env.${VITE_NODE_ENV} vite",
    "start-env": "VITE_NODE_ENV=dev npm run start",
    "start-staging": "VITE_NODE_ENV=staging npm run start",
    "start-pro": "VITE_NODE_ENV=pro npm run start",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore && echo \"Tai Dev Check Eslint âœ…\"",
    "lint:fix": "eslint . --fix --ext .ts,.vue",
    "prettier": "prettier --write",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "prepare": "husky install",
    "pre-commit": "echo \"[Husky] pre-commit\"",
    "lint-staged": "lint-staged",
    "release": "standard-version"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "vite": "^4.4.5"
  }
}
